---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<sitemapindex xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <sitemap>
    <loc>https://www.johnsy.com/sitemap.xml</loc>
  </sitemap>
  {% for utility in site.data.parkrun-utilities.utilities %}
  {% if utility.url contains 'www.johnsy.com' and utility.url contains '/' %}
  <sitemap>
    <loc>{{ utility.url }}sitemap.xml</loc>
  </sitemap>
  {% endif %}
  {% endfor %}
  {% for microsite in site.data.microsites.microsites %}
  {% if microsite.url contains 'www.johnsy.com' and microsite.url contains '/' %}
  <sitemap>
    <loc>{{ microsite.url }}sitemap.xml</loc>
  </sitemap>
  {% endif %}
  {% endfor %}
</sitemapindex>
